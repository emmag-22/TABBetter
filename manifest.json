{
  "manifest_version": 3,
  "name": "TABBetter",
  "description": "AI-powered tab organization for focus and efficiency.",
  "version": "0.1.1",

  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    }
  },

  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },

  "permissions": ["tabs", "tabGroups"],
  "background": {
    "service_worker": "background.js"
  }
}
{
  "name": "my-cursor-project",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build"
  }
}
